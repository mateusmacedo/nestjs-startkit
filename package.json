{
  "name": "nestjs-startkit",
  "version": "0.1.0",
  "description": "NestJS Startkit",
  "author": {
    "name": "Mateus Macedo Dos Anjos",
    "email": "macedodosanjosmateus@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mateusmacedo/nestjs-startkit.git"
  },
  "bugs": {
    "url": "https://github.com/mateusmacedo/nestjs-startkit/issues",
    "email": "macedodosanjosmateus@gmail.com"
  },
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "check": "npx npm-check -s -u",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "export NODE_ENV=development && ts-node-dev --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register ./src/main.ts --inspect=0.0.0.0:9229",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:staged": "eslint --fix",
    "test": "export NODE_ENV=test && export TS_NODE_DEV=true && node --trace-warnings -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --passWithNoTests --maxWorkers=4 --no-cache --detectOpenHandles --forceExit",
    "test:unit": "npm test -- --config ./jest-unit.config.js",
    "test:staged": "npm test -- --config ./jest-unit.config.js --findRelatedTests",
    "test:watch": "npm test -- --watch --config ./jest-unit.config.js",
    "test:cov": "npm test -- --coverage --config ./jest-unit.config.js",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --passWithNoTests --runInBand --no-cache --config ./jest-unit.config.js",
    "test:e2e": "npm test -- --config ./jest-e2e.config.js",
    "release": "standard-version",
    "changelog": "standard-version && auto-changelog -p -u --commit-limit false --hide-credit --template changelog-template.hbs",
    "changelog:minor": "standard-version --release-as minor && auto-changelog -p -u --commit-limit false --hide-credit --template changelog-template.hbs",
    "changelog:major": "standard-version --release-as major && auto-changelog -p -u --commit-limit false --hide-credit --template changelog-template.hbs",
    "changelog:patch": "standard-version --release-as patch && auto-changelog -p -u --commit-limit false --hide-credit --template changelog-template.hbs",
    "changelog:alpha": "standard-version --prerelease alpha && auto-changelog -p -u --commit-limit false --hide-credit --template changelog-template.hbs"
  },
  "dependencies": {
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.1",
    "@nestjs/terminus": "^9.1.1",
    "@nestjs/typeorm": "^9.0.1",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "joi": "^17.6.0",
    "module-alias": "^2.2.2",
    "mongodb": "^3.7.3",
    "redis": "^4.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.9",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@jest-mock/express": "^2.0.1",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cache-manager": "^4.0.2",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.3",
    "@types/jest": "28.1.8",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "auto-changelog": "^2.4.0",
    "commitlint": "^17.1.2",
    "coveralls": "^3.1.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.1",
    "faker": "5.5.3",
    "faker-br": "^0.4.1",
    "git-commit-msg-linter": "^4.1.3",
    "git-cz": "^4.9.0",
    "husky": "^8.0.1",
    "jest": "^28.0.8",
    "jest-mock-extended": "^2.0.7",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^13.0.3",
    "mockdate": "^3.0.5",
    "mongodb-memory-server": "^8.9.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "standard-version": "^9.5.0",
    "supertest": "^6.1.3",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.2"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "standard-version": {
    "skip": {
      "changelog": true,
      "commit": true
    }
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  }
}
